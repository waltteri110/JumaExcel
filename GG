@@ -0,0 +1,19 @@
+#!/usr/bin/env bash
+set -euo pipefail
+
+ROOT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
+TARGET_DIR="${1:-$HOME/Downloads}"
+
+python3 "$ROOT_DIR/generate_excel_files.py" --output-dir "$ROOT_DIR"
+
+# Luo kohdekansio, jos mahdollista.
+mkdir -p "$TARGET_DIR"
+
+SRC="$ROOT_DIR/Tuotannon_arjen_Excel_paketti_v1.zip"
+DST="$TARGET_DIR/Tuotannon_arjen_Excel_paketti_v1.zip"
+
+if [ "$(realpath "$SRC")" != "$(realpath "$DST")" ]; then
+  cp -f "$SRC" "$DST"
+fi
+
+printf '\nValmis! Ladattava tiedosto:\n%s\n' "$DST"
diff --git a/generate_excel_files.py b/generate_excel_files.py
new file mode 100644
index 0000000000000000000000000000000000000000..847548a46b02365e0b8d43904917fe3e7c1b5cdc
--- /dev/null
+++ b/generate_excel_files.py
@@ -0,0 +1,197 @@
+from __future__ import annotations
+import argparse
+from datetime import datetime, date, time
+from pathlib import Path
+from zipfile import ZipFile, ZIP_DEFLATED
+from xml.sax.saxutils import escape
+
+ROOT = Path(__file__).resolve().parent
+
+
+def col_letter(n: int) -> str:
+    s = ''
+    while n:
+        n, r = divmod(n - 1, 26)
+        s = chr(65 + r) + s
+    return s
+
+
+def excel_date(d: date) -> int:
+    epoch = date(1899, 12, 30)
+    return (d - epoch).days
+
+
+def excel_time(t: time) -> float:
+    return (t.hour * 3600 + t.minute * 60 + t.second) / 86400
+
+
+def c_inline(ref: str, text: str) -> str:
+    return f'<c r="{ref}" t="inlineStr"><is><t>{escape(text)}</t></is></c>'
+
+
+def c_num(ref: str, value: float | int, style: int | None = None) -> str:
+    s = f' s="{style}"' if style is not None else ''
+    return f'<c r="{ref}"{s}><v>{value}</v></c>'
+
+
+def c_formula(ref: str, formula: str, style: int | None = None) -> str:
+    s = f' s="{style}"' if style is not None else ''
+    return f'<c r="{ref}"{s}><f>{escape(formula)}</f></c>'
+
+
+def row_xml(r: int, cells: list[str]) -> str:
+    return f'<row r="{r}">' + ''.join(cells) + '</row>'
+
+
+STYLES_XML = '''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<styleSheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main">
+  <numFmts count="2">
+    <numFmt numFmtId="164" formatCode="dd.mm.yyyy"/>
+    <numFmt numFmtId="165" formatCode="hh:mm"/>
+  </numFmts>
+  <fonts count="1"><font><sz val="11"/><name val="Calibri"/><family val="2"/></font></fonts>
+  <fills count="1"><fill><patternFill patternType="none"/></fill></fills>
+  <borders count="1"><border><left/><right/><top/><bottom/><diagonal/></border></borders>
+  <cellStyleXfs count="1"><xf numFmtId="0" fontId="0" fillId="0" borderId="0"/></cellStyleXfs>
+  <cellXfs count="3">
+    <xf numFmtId="0" fontId="0" fillId="0" borderId="0" xfId="0"/>
+    <xf numFmtId="164" fontId="0" fillId="0" borderId="0" xfId="0" applyNumberFormat="1"/>
+    <xf numFmtId="165" fontId="0" fillId="0" borderId="0" xfId="0" applyNumberFormat="1"/>
+  </cellXfs>
+  <cellStyles count="1"><cellStyle name="Normal" xfId="0" builtinId="0"/></cellStyles>
+</styleSheet>'''
+
+
+def make_viikkoseuranta(path: Path):
+    headers = ['Päivämäärä','Viikko','Tiimi','Tuote_Työvaihe','Suunniteltu_maara','Toteutunut_maara','Poikkeama','Poikkeaman_syy']
+    data = [
+        ('17.02.2026','Tiimi A','477S-EMV',30,28,'Materiaalipula'),('17.02.2026','Tiimi B','479S-Hitsaus',22,22,'Muu'),
+        ('18.02.2026','Tiimi A','477S-Maalaus',15,13,'Konevika'),('18.02.2026','Tiimi C','481S-Kokoonpano',18,19,'Muu'),
+        ('19.02.2026','Tiimi B','479S-EMV',20,17,'Henkilöstövajaus'),('19.02.2026','Tiimi C','482S-Hitsaus',16,16,'Muu'),
+        ('20.02.2026','Tiimi A','477S-Tarkastus',14,14,'Muu'),('20.02.2026','Tiimi B','483S-EMV',24,20,'Suunnitteluvirhe'),
+        ('21.02.2026','Tiimi C','484S-Asennus',12,11,'Konevika'),('21.02.2026','Tiimi A','485S-Hitsaus',17,18,'Muu'),
+        ('22.02.2026','Tiimi B','486S-EMV',19,19,'Muu'),('22.02.2026','Tiimi C','487S-Maalaus',21,18,'Materiaalipula'),
+        ('23.02.2026','Tiimi A','488S-Kokoonpano',23,21,'Henkilöstövajaus'),('23.02.2026','Tiimi B','489S-Tarkastus',13,13,'Muu'),
+        ('24.02.2026','Tiimi C','490S-EMV',26,24,'Konevika'),('24.02.2026','Tiimi A','491S-Hitsaus',18,17,'Muu'),
+        ('25.02.2026','Tiimi B','492S-Asennus',20,16,'Suunnitteluvirhe'),('25.02.2026','Tiimi C','493S-Maalaus',15,15,'Muu'),
+        ('26.02.2026','Tiimi A','494S-Tarkastus',16,14,'Materiaalipula'),('26.02.2026','Tiimi B','495S-EMV',22,23,'Muu')
+    ]
+    rows = [row_xml(1, [c_inline(f'{col_letter(i)}1', h) for i, h in enumerate(headers, 1)])]
+    for r, (d, tiimi, tuote, s, t, syy) in enumerate(data, start=2):
+        dd = datetime.strptime(d, '%d.%m.%Y').date()
+        rows.append(row_xml(r, [
+            c_num(f'A{r}', excel_date(dd), 1),
+            c_formula(f'B{r}', f'ISOWEEKNUM(A{r})'),
+            c_inline(f'C{r}', tiimi),
+            c_inline(f'D{r}', tuote),
+            c_num(f'E{r}', s),
+            c_num(f'F{r}', t),
+            c_formula(f'G{r}', f'F{r}-E{r}'),
+            c_inline(f'H{r}', syy),
+        ]))
+    for r in range(22, 501):
+        rows.append(row_xml(r, [c_formula(f'B{r}', f'ISOWEEKNUM(A{r})'), c_formula(f'G{r}', f'F{r}-E{r}')]))
+
+    sheet1 = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main">
+  <dimension ref="A1:H500"/>
+  <sheetData>{''.join(rows)}</sheetData>
+  <dataValidations count="1"><dataValidation type="list" allowBlank="1" sqref="H2:H500"><formula1>"Materiaalipula,Konevika,Henkilöstövajaus,Suunnitteluvirhe,Muu"</formula1></dataValidation></dataValidations>
+</worksheet>'''
+
+    s2rows = [
+        row_xml(1,[c_inline('A1','Valittu viikko'),c_num('B1',8)]),
+        row_xml(3,[c_inline('A3','Suunniteltu yhteensä'),c_formula('B3','SUMIFS(DataSyotto!E:E,DataSyotto!B:B,$B$1)')]),
+        row_xml(4,[c_inline('A4','Toteutunut yhteensä'),c_formula('B4','SUMIFS(DataSyotto!F:F,DataSyotto!B:B,$B$1)')]),
+        row_xml(5,[c_inline('A5','Poikkeama yhteensä'),c_formula('B5','SUMIFS(DataSyotto!G:G,DataSyotto!B:B,$B$1)')]),
+        row_xml(6,[c_inline('A6','Rivejä viikolla'),c_formula('B6','COUNTIF(DataSyotto!B:B,$B$1)')]),
+    ]
+    sheet2 = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"><sheetData>{''.join(s2rows)}</sheetData></worksheet>'''
+
+    s3rows=[row_xml(1,[c_inline('A1','Käyttöohje (2 min alkuun)')]),row_xml(3,[c_inline('A3','1) Syötä tuotantorivit DataSyotto-välilehdelle.')]),row_xml(4,[c_inline('A4','2) Syötä Viikkonakyma!B1 kenttään haluttu viikkonumero.')]),row_xml(5,[c_inline('A5','3) Lue yhteenveto: suunniteltu, toteutunut, poikkeama ja rivimäärä.')])]
+    sheet3 = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"><sheetData>{''.join(s3rows)}</sheetData></worksheet>'''
+
+    write_workbook(path, ['DataSyotto','Viikkonakyma','Ohje'], [sheet1,sheet2,sheet3])
+
+
+def make_vuorot(path: Path):
+    headers=['Nimi','Paiva','Vuoro_alkaa','Vuoro_paattyy','Tauko_h','Tehdyt_tunnit','Iltalisa_tunnit_18_22','Yolisatunnit_22_06']
+    data=[('Waltteri','17.02.2026','06:00','14:00',0.5),('Mikko','17.02.2026','13:30','21:30',0.5),('Jari','17.02.2026','18:00','23:00',0.5),('Sami','18.02.2026','22:00','06:00',0.5),('Antti','18.02.2026','14:00','22:00',0.5),('Waltteri','19.02.2026','12:00','20:00',0.5),('Mikko','19.02.2026','21:00','05:00',0.5),('Jari','20.02.2026','08:00','16:00',0.5),('Sami','20.02.2026','16:00','00:00',0.5),('Antti','21.02.2026','23:00','07:00',0.5)]
+    rows=[row_xml(1,[c_inline(f'{col_letter(i)}1',h) for i,h in enumerate(headers,1)])]
+    for r,(n,d,a,p,t) in enumerate(data,start=2):
+        dd=datetime.strptime(d,'%d.%m.%Y').date(); ta=datetime.strptime(a,'%H:%M').time(); tp=datetime.strptime(p,'%H:%M').time()
+        rows.append(row_xml(r,[c_inline(f'A{r}',n),c_num(f'B{r}',excel_date(dd),1),c_num(f'C{r}',excel_time(ta),2),c_num(f'D{r}',excel_time(tp),2),c_num(f'E{r}',t),c_formula(f'F{r}',f'IF(D{r}<C{r},(D{r}+1-C{r})*24-E{r},(D{r}-C{r})*24-E{r})'),c_formula(f'G{r}',f'IF(D{r}>=C{r},MAX(0,MIN(D{r},TIME(22,0,0))-MAX(C{r},TIME(18,0,0)))*24,MAX(0,TIME(22,0,0)-MAX(C{r},TIME(18,0,0)))*24)'),c_formula(f'H{r}',f'IF(D{r}>=C{r},MAX(0,MIN(D{r},TIME(6,0,0))-C{r})*24 + MAX(0,D{r}-TIME(22,0,0))*24,MAX(0,TIME(6,0,0)-C{r})*24 + MAX(0,D{r})*24 + MAX(0,TIME(24,0,0)-MAX(C{r},TIME(22,0,0)))*24)')]))
+    for r in range(12,501):
+        rows.append(row_xml(r,[c_formula(f'F{r}',f'IF(D{r}<C{r},(D{r}+1-C{r})*24-E{r},(D{r}-C{r})*24-E{r})'),c_formula(f'G{r}',f'IF(D{r}>=C{r},MAX(0,MIN(D{r},TIME(22,0,0))-MAX(C{r},TIME(18,0,0)))*24,MAX(0,TIME(22,0,0)-MAX(C{r},TIME(18,0,0)))*24)'),c_formula(f'H{r}',f'IF(D{r}>=C{r},MAX(0,MIN(D{r},TIME(6,0,0))-C{r})*24 + MAX(0,D{r}-TIME(22,0,0))*24,MAX(0,TIME(6,0,0)-C{r})*24 + MAX(0,D{r})*24 + MAX(0,TIME(24,0,0)-MAX(C{r},TIME(22,0,0)))*24)')]))
+    sheet1=f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?><worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"><dimension ref="A1:H500"/><sheetData>{''.join(rows)}</sheetData></worksheet>'''
+
+    s2=[]
+    s2.append(row_xml(1,[c_inline('A1','Iltalisa_e_h'),c_num('B1',3.73)]))
+    s2.append(row_xml(2,[c_inline('A2','Yolisa_e_h'),c_num('B2',4.40)]))
+    heads=['Nimi','Tehdyt_tunnit','Iltalisa_tunnit','Yolisatunnit','Iltalisa_euroa','Yolisa_euroa','Lisat_yhteensa']
+    s2.append(row_xml(5,[c_inline(f'{col_letter(i)}5',h) for i,h in enumerate(heads,1)]))
+    for r,n in enumerate(['Waltteri','Mikko','Jari','Sami','Antti'],start=6):
+        s2.append(row_xml(r,[c_inline(f'A{r}',n),c_formula(f'B{r}',f'SUMIFS(Vuorot!F:F,Vuorot!A:A,$A{r})'),c_formula(f'C{r}',f'SUMIFS(Vuorot!G:G,Vuorot!A:A,$A{r})'),c_formula(f'D{r}',f'SUMIFS(Vuorot!H:H,Vuorot!A:A,$A{r})'),c_formula(f'E{r}',f'C{r}*$B$1'),c_formula(f'F{r}',f'D{r}*$B$2'),c_formula(f'G{r}',f'E{r}+F{r}')]))
+    sheet2=f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?><worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"><sheetData>{''.join(s2)}</sheetData></worksheet>'''
+
+    s3=[row_xml(1,[c_inline('A1','Käyttöohje')]),row_xml(3,[c_inline('A3','1) Syötä vuorot Vuorot-välilehdelle (ajat muodossa hh:mm).')]),row_xml(4,[c_inline('A4','2) Syötä tauko tunteina (esim. 0,5).')]),row_xml(5,[c_inline('A5','3) Tarkista lisien eurot Lisalaskuri-välilehdeltä.')])]
+    sheet3=f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?><worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"><sheetData>{''.join(s3)}</sheetData></worksheet>'''
+
+    write_workbook(path, ['Vuorot','Lisalaskuri','Ohje'], [sheet1,sheet2,sheet3])
+
+
+def write_workbook(path: Path, sheet_names: list[str], sheets_xml: list[str]):
+    wb_sheets = ''.join([f'<sheet name="{escape(name)}" sheetId="{i}" r:id="rId{i}"/>' for i, name in enumerate(sheet_names, 1)])
+    workbook = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<workbook xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main" xmlns:r="http://schemas.openxmlformats.org/officeDocument/2006/relationships"><sheets>{wb_sheets}</sheets></workbook>'''
+    rels = ''.join([f'<Relationship Id="rId{i}" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/worksheet" Target="worksheets/sheet{i}.xml"/>' for i in range(1, len(sheet_names)+1)])
+    wb_rels = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<Relationships xmlns="http://schemas.openxmlformats.org/package/2006/relationships">{rels}<Relationship Id="rId{len(sheet_names)+1}" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/styles" Target="styles.xml"/></Relationships>'''
+    ct_over = ''.join([f'<Override PartName="/xl/worksheets/sheet{i}.xml" ContentType="application/vnd.openxmlformats-officedocument.spreadsheetml.worksheet+xml"/>' for i in range(1, len(sheet_names)+1)])
+    content_types = f'''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<Types xmlns="http://schemas.openxmlformats.org/package/2006/content-types"><Default Extension="rels" ContentType="application/vnd.openxmlformats-package.relationships+xml"/><Default Extension="xml" ContentType="application/xml"/>{ct_over}<Override PartName="/xl/workbook.xml" ContentType="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet.main+xml"/><Override PartName="/xl/styles.xml" ContentType="application/vnd.openxmlformats-officedocument.spreadsheetml.styles+xml"/></Types>'''
+    root_rels = '''<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<Relationships xmlns="http://schemas.openxmlformats.org/package/2006/relationships"><Relationship Id="rId1" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/officeDocument" Target="xl/workbook.xml"/></Relationships>'''
+
+    with ZipFile(path, 'w', compression=ZIP_DEFLATED) as z:
+        z.writestr('[Content_Types].xml', content_types)
+        z.writestr('_rels/.rels', root_rels)
+        z.writestr('xl/workbook.xml', workbook)
+        z.writestr('xl/_rels/workbook.xml.rels', wb_rels)
+        z.writestr('xl/styles.xml', STYLES_XML)
+        for i, sx in enumerate(sheets_xml, 1):
+            z.writestr(f'xl/worksheets/sheet{i}.xml', sx)
+
+
+def main() -> None:
+    parser = argparse.ArgumentParser(description='Generoi tuotannon Excel MVP -paketti (.xlsx + .zip).')
+    parser.add_argument(
+        '--output-dir',
+        default=str(ROOT),
+        help='Kansio, johon tiedostot luodaan (oletus: skriptin kansio).',
+    )
+    args = parser.parse_args()
+
+    out_dir = Path(args.output_dir).resolve()
+    out_dir.mkdir(parents=True, exist_ok=True)
+
+    viikkoseuranta = out_dir / 'Viikkoseuranta_v1.xlsx'
+    vuorot = out_dir / 'Vuorot_ja_lisat_v1.xlsx'
+    paketti = out_dir / 'Tuotannon_arjen_Excel_paketti_v1.zip'
+
+    make_viikkoseuranta(viikkoseuranta)
+    make_vuorot(vuorot)
+
+    with ZipFile(paketti, 'w', compression=ZIP_DEFLATED) as z:
+        z.write(viikkoseuranta, viikkoseuranta.name)
+        z.write(vuorot, vuorot.name)
+
+    print(f'Luotu: {viikkoseuranta}')
+    print(f'Luotu: {vuorot}')
+    print(f'Luotu: {paketti}')
+
+
+if __name__ == '__main__':
+    main()
 
EOF
)
